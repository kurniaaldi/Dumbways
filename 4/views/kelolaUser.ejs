        crossorigin="anonymous"></script>

    <title>Me-Dumb</title>
</head>

<body>

    <nav class="navbar navbar-light bg-light">
        <a class="navbar-brand" href="/users">
            <img src="#" width="30" height="30" class="d-inline-block align-top" alt="">
            Aldi News
        </a>
    </nav>
    <div class="container mt-5">
        <div class="row">

            <% result.forEach(function(e){ %>
            <div class="card mr-3 mt-3" style="width: 18rem;">
                <a href="/users/kelolaUser/<%= e.id %>/update">
                    <button class="datasett" data-id="<%= e.id %>">Edit</button>
                </a>
                <form action="/users/kelolaUser/<%= e.id %>?_method=DELETE" method="POST">
                    <button type="submit">hapus<%= e.id %></button>
                </form>
                <div data-spy="scroll responsive" data-target="#navbar-example3" data-offset="0">

                    <ul class="list-group list-group-flush">
                        <li class="list-group-item"><%= e.name %> </li>
                        <li class="list-group-item"><%= e.email %></li>
                    </ul>
                </div>
            </div>
            <% } ) %>
        </div>
    </div>









</body>

</html>